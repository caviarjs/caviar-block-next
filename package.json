{
  "name": "@caviar/next-block",
  "version": "4.1.2",
  "description": "The official caviar block for next",
  "main": "src/index.js",
  "scripts": {
    "test": "NODE_DEBUG=@caviar/next-block nyc ava --timeout=10s --verbose",
    "test:dev": "NODE_DEBUG=@caviar/next-block nyc ava --timeout=10s --verbose && npm run report:dev",
    "lint": "eslint .",
    "fix": "eslint . --fix",
    "posttest": "npm run report",
    "report": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "report:dev": "nyc report --reporter=html && npm run report:open",
    "report:open": "open coverage/index.html"
  },
  "files": [
    "src/"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/caviarjs/next-block.git"
  },
  "keywords": [
    "@caviar/next-block",
    "caviar-block",
    "caviar",
    "next",
    "operation"
  ],
  "engines": {
    "node": ">= 6"
  },
  "ava": {
    "babel": false,
    "files": [
      "test/*.test.js"
    ]
  },
  "author": "caviarjs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/caviarjs/next-block/issues"
  },
  "devDependencies": {
    "@caviar/test": "^2.0.1",
    "@ostai/eslint-config": "^3.5.0",
    "ava": "^2.4.0",
    "caviar": "^6.0.1",
    "codecov": "^3.5.0",
    "eslint": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "express": "^4.17.1",
    "koa": "^2.8.1",
    "next": "^9.3.3",
    "nyc": "^14.1.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "test-fixture": "^2.4.1",
    "webpack": "^4.40.2"
  },
  "dependencies": {
    "compose-middleware": "^5.0.1",
    "connect-mount": "^1.0.0",
    "core-util-is": "^1.0.2",
    "err-object": "^5.1.4",
    "express-to-koa": "^2.0.0",
    "next-compose-plugins": "^2.2.0",
    "pre-suf": "^1.1.1",
    "require-esmodule": "^1.0.1",
    "resolve-from": "^5.0.0",
    "serve-static": "^1.14.1",
    "supertest": "^4.0.2",
    "tapable": "^1.1.3"
  },
  "peerDependencies": {
    "caviar": "^6.0.1",
    "next": "^9.3.3"
  }
}
